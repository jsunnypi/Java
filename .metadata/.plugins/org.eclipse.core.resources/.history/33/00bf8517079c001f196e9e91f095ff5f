package Ex;

import java.util.*;

public class VectorRain {
	Vector<Integer> v = new Vector<Integer>();
	Scanner sc = new Scanner(System.in);
	
	private void read() {
		System.out.println("강수량 입력 (0 입력 시 종료)>> ");int r = sc.nextInt();
		for(int i=0;i<v.size();i++) {
			
//			v.add(r);
			int n = v.get(i);
			System.out.println(n);
			if(r==0)
				break;
		}
		
		
	}
	
	public void run() {
		read();
	}
	
	public static void main(String[] args) {
		VectorRain rain = new VectorRain();
		rain.run();
	}

}
